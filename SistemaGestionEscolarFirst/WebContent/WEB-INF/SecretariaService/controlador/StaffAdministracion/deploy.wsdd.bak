<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from SecretariaService WSDL service -->

  <service name="Secretaria" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://StaffAdministracion.controlador"/>
      <parameter name="wsdlServiceElement" value="SecretariaService"/>
      <parameter name="schemaQualified" value="http://StaffAdministracion.controlador"/>
      <parameter name="wsdlServicePort" value="Secretaria"/>
      <parameter name="className" value="controlador.StaffAdministracion.SecretariaSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="Secretaria"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="agregarNuevaSecretatia" qname="operNS:agregarNuevaSecretatia" xmlns:operNS="http://StaffAdministracion.controlador" returnQName="retNS:agregarNuevaSecretatiaReturn" xmlns:retNS="http://StaffAdministracion.controlador" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:nuevaPersona" xmlns:pns="http://StaffAdministracion.controlador" type="tns:anyType" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="agregarNuevaSecretatia"/>

      <typeMapping
        xmlns:ns="http://StaffAdministracion.controlador"
        qname="ns:>agregarNuevaSecretatiaResponse"
        type="java:controlador.StaffAdministracion.AgregarNuevaSecretatiaResponse"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
  </service>
</deployment>
